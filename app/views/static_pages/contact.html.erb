<header class="page-header">
  <h1 class="page-heading text-center">Contact</h1>
</header>

<%= image_tag "gh_logo" %>
<a href="https://github.com/garedean" class="github-link" target="_blank">https://github.com/garedean</a>

<div id="contact-wrapper">
  I'd love to hear from you! Send me a message:<br>
  <%= bootstrap_form_for "#" do |f| %>
    <%= f.text_field :name %>
    <%= f.text_area :message %>
    <%= f.submit %>
  <% end %>
</div>
